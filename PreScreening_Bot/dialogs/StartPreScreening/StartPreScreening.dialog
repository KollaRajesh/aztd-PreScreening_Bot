              ]
            },
            {
              "$kind": "Microsoft.IfCondition",
              "$designer": {
                "id": "VeUWcC"
              },
              "condition": "=(dialog.NoOfCorrectAnswers >=dialog.RequiredCount)",
              "actions": [
                {
                  "$kind": "Microsoft.SendActivity",
                  "$designer": {
                    "id": "iXr8nD"
                  },
                  "activity": "${SendActivity_iXr8nD()}"
                }
              ],
              "elseActions": [
                {
                  "$kind": "Microsoft.SendActivity",
                  "$designer": {
                    "id": "QPJYO8"
                  },
                  "activity": "${SendActivity_QPJYO8()}"
                }
              ]
            },
            {
              "$kind": "Microsoft.SendActivity",
              "$designer": {
                "id": "5MCLrU"
              },
              "activity": "${SendActivity_5MCLrU()}"
            }
          ],
          "elseActions": [
            {
              "$kind": "Microsoft.SendActivity",
              "$designer": {
                "id": "xEodPm"
              },
              "activity": "${SendActivity_xEodPm()}"
