              "property": "dialog.QnAObj",
              "value": {
                "TotalNoOfQuestions": 5,
                "Questions": [
                  {
                    "Question": "1.Which of the following is a PaaS example?",
                    "Options": [
                      "Microsoft Power BI Embedded",
                      "Azure Key Vault",
                      "Azure Firewall",
                      "Azure DevOps"
                    ],
                    "Answer": "Azure Key Vault"
                  },
                  {
                    "Question": "2.Which of the following best describes Azure Service Bus?",
                    "Options": [
                      "It's a network bus that carries traffic for all Azure services",
                      "It's a set of APIs to access Azure services",
                      "It's a message broker",
                      "It's an event streaming service"
                    ],
                    "Answer": "It's a message broker"
                  },
                  {
                    "Question": "3.TCP is an example of which OSI layer?",
                    "Options": [
                      "Transport",
                      "Network",
                      "DataLink",
                      "Session"
                    ],
                    "Answer": "Transport"
                  },
                  {
                    "Question": "4.True or False , Azure Kubernetes Services has far less adminstrative overhead and provides quicker deployment of containers to cloud when compared to Azure Container Instances?",
                    "Options": [
                      "True",
                      "False"
                    ],
                    "Answer": "False"
                  },
                  {
                    "Question": "5.Which Azure service should use to store certificates?",
                    "Options": [
                      "Azure Storage Account",
                      "Azure Security Centre",
                      "Azure Active Directory",
                      "Azure Key vault"
                    ],
                    "Answer": "Azure Key vault"
                  }
                ]
              }
            },
            {
              "property": "dialog.NoOfQuestions",
              "value": 0
            },
            {
              "property": "dialog.NoOfWrongAnswers",
              "value": 0
            },
            {
              "property": "dialog.NoOfCorrectAnswers",
              "value": 0
            },
            {
              "property": "dialog.TotalNoOfQuestions",
              "value": "=dialog.QnAObj.TotalNoOfQuestions"
            },
            {
              "value": "=add(div(dialog.TotalNoOfQuestions, 2),1)",
              "property": "dialog.RequiredCount"
            }
          ]
        },
        {
          "$kind": "Microsoft.SendActivity",
          "$designer": {
            "id": "JtEwWm"
